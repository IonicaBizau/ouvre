{
  "name": "ouvre",
  "version": "1.2.10",
  "description": "Open web pages configured in a config file to be more efficient.",
  "main": "bin/ouvre.sh",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "ouvre": "bin/ouvre.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IonicaBizau/ouvre.git"
  },
  "keywords": [
    "open",
    "web",
    "page",
    "shell",
    "node"
  ],
  "author": "Ionică Bizău <bizauionica@gmail.com> (https://ionicabizau.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IonicaBizau/ouvre/issues"
  },
  "homepage": "https://github.com/IonicaBizau/ouvre",
  "blah": {
    "show_docs": false,
    "installation": [
      {
        "h2": "Configuring the `~/.ouvre.conf` file"
      },
      {
        "p": "Create a file named `.ouvre.conf` in your home directory containing urls - one per line:"
      },
      {
        "code": {
          "language": "sh",
          "content": [
            "# Social",
            "https://www.facebook.com/",
            "https://twitter.com/",
            "",
            "# Programming",
            "http://stackoverflow.com/users/1420197/",
            "https://github.com/",
            "",
            "# My Website",
            "http://ionicabizau.net",
            "",
            "# e-Mail",
            "https://mail.google.com/mail/u/0/#inbox",
            "https://mail.google.com/mail/u/1/#inbox",
            "",
            "# ...and so on"
          ]
        }
      },
      {
        "p": "Then, running `ouvre`, the default browser will open the links."
      }
    ]
  },
  "files": [
    "bin/",
    "app/",
    "lib/",
    "dist/",
    "src/",
    "scripts/",
    "resources/",
    "menu/",
    "cli.js",
    "index.js",
    "bloggify.js",
    "bloggify.json",
    "bloggify/"
  ]
}
